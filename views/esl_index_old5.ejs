                // make a clone object
                for (let i = 0; i < viewInfo.length; i++) {
                    const curViewInfo = viewInfo[i];
                    let view = makeView(i, curViewInfo);

                    // apply property
                    for (let j = 0; j < curViewInfo.properties.length; j++) {
