                        console.log('Mode switched from', oldMode, 'to', newMode);
                    }
                };
                jsonEditor = new JSONEditor(JsonContainer, options);
                jsonEditor.set({});
                //jsonEditor.expandAll();
                 
            }
            

            function initJsonEditor2() {
                // create the editor
