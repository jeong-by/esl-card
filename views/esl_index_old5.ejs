            .dropzone {
                background-color: #ccc;
                border: dashed 4px transparent;
                transition: background-color 0.3s;
            }

            .drop-active {
                border-color: #aaa;
            }

            .drop-target {
                background-color: #29e;
                border-color: #fff;
                border-style: solid;
            }

            .palette {
                display: table;
            }

            .palette-row {
                display: table-row;
            }

            /* 갖다 놓을 아이템 */
            .drag-drop {
                display: table-cell;
                width: 7em;
                height: 3em;
                color: #fff;
                background-color: #29e;
                border: solid 2px #fff;
                touch-action: none;
                -webkit-transform: translate(0px, 0px);
