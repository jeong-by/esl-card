            function initJsonEditor() {
                // create the editor
                var JsonContainer = document.getElementById("JsonContainer");
                var options = {
                    mode: 'code',
                    modes: ['code', 'tree'], // allowed modes
                    onError: function (err) {
                        alert(err.toString());
                    },
